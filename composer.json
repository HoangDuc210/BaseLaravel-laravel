{
  "name": "baselaravel/laravel",
  "description": "Supercharged service, controller, repository in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "controller",
    "service",
    "repository"
  ],
  "authors": [
    {
      "name": "Hoang Nguyen",
      "email": "nguyenduchoang12c5@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "prettus/l5-repository": "^2.10",
    "illuminate/support": "5.8.*||^6.0||^7.0||^8.0||^9.0||^10.0||^11.0||^12.0",
    "psr/simple-cache": "^1.0||^2.0||^3.0",
    "composer/semver": "^3.3"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "BaselaravelLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BaselaravelLaravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [],
      "aliases": {
        "SessionFormRequestTrait": "BaselaravelLaravel\\Controllers\\Traits",
        "HasCompositePrimaryKey": "BaselaravelLaravel\\Models\\Traits",
        "Recordable": "BaselaravelLaravel\\Models\\Traits",
        "BaseRepository": "BaselaravelLaravel\\Repositories\\Concerns",
        "BaseService": "BaselaravelLaravel\\Services\\Concerns",
        "SearchBaseServiceAbstract": "BaselaravelLaravel\\Services\\Concerns",
        "MessageTraitService": "BaselaravelLaravel\\Services\\Traits"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}